<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>JavaScript Function Passed as Argument to Other Function</title>
</head>
<body>
    <script>
    function test()
    {
      var Marco = "Marco";  

      return "Ciao" + Marco;
    }

     function test(){
       var risposta = prompt("vuoi iniziare un'avventura?"); 
        if (risposta = "si") {
          return "grandioso che entusiasmo";

            }
       
      if {risposta = "no"){
          return "va bene torna quando ne avrai voglia";

            }

       else {  return risposta;
            }

     }

    var stampa = test() + risposta(); 


    document.write(stampa); // Output: Ciao, Marco
    </script>
</body>
</html>
